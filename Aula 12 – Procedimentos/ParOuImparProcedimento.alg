algoritmo "ParOuImparProcedimento"
var
   N: Inteiro

Procedimento ParOuImpar(V: Inteiro)
  Inicio
    Se (V%2 = 0) entao
       Escreval(V, " -> Par!")
    Senao
       Escreval(V, " -> Impar!")
    FimSe
  FimProcedimento

inicio
      Escreva("Digite um numero: ")
      Leia(N)
      ParOuImpar(N)





fimalgoritmo
