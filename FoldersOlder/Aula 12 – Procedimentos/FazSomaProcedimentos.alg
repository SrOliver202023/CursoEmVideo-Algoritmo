algoritmo "FazSomaProcedimento"
var
   X, Y: Inteiro

Procedimento Soma(A, B: Inteiro)
  Inicio
  
    Escreval("      ______________")
    Escreval("      Valor A: ", A)
    Escreval("      Valor B: ", B)
    Escreval("      ______________")
    Escreval("     ",A, " + ", B ," = ", A+B)

  FimProcedimento
inicio

      X <- 5
      Y <- 5


      Soma(X, Y)


fimalgoritmo
